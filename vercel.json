{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.fastrakconnect.com/application/:path*"
    },
    {
      "source": "/",
      "destination": "/jobs",
      "has": [
        {
          "type": "host",
          "value": "jobs.fastrakconnect.com"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
